package bitcamp.pms;

import bitcamp.pms.annotation.Component;

public class Practise0 {

  public static void main(String[] args) throws ClassNotFoundException {
    Class<?> c = Class.forName("bitcamp.pms.ProjectApp");
    System.out.println(c);
    
    String str = String.class.getName();
    System.out.println(str); // java.lang.String
    String str1 = byte.class.getName();
    System.out.println(str1); // byte
    
    Component anno = clazz.getAnnotation(Component.class);
    String key = anno.value();
    if (key.equals("")) {
      key = clazz.getName();
    }
    objPool.put(key, clazz.newInstance());
    
  }
}
