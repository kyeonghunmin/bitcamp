<mapper namespace="step30.exam01.MemberDao">
  
  <select id="selectList" resultType="step30.exam01.Member">
    select
      MNO as no,
      MNAME as name,
      EMAIL,
      PWD as password,
      TEL
    from MEMBERS
  </select>
</mapper>