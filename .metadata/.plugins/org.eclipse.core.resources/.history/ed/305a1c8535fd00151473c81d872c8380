<mapper namespace="step30.exam02.MemberDao">

  <!-- 
  insert문은 insert 태그에 작성한다.
  update 태그나 delete 태그에 작성해도 동작한다.
  그러나 유지보수를 위해서 적확하게 사용하라
   -->
  <insert id="insert">
  
  </insert>
  
  <select id="selectList" resultType="step30.exam02.Member">
    select
      MNO as no,
      MNAME as name,
      EMAIL,
      PWD as password,
      TEL
    from MEMBERS
  </select>
</mapper>